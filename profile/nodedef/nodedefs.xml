<nodeDefs>
    <!-- controller -->
    <nodeDef id="russound" nodeType="139" nls="ctl">
        <editors />
        <sts>
		<st id="ST" editor="bool" />
		<st id="GV0" editor="bool" />
	</sts>
    <cmds>
		<sends />
		<accepts>
			<cmd id="DISCOVER" />
        	<cmd id="REMOVE_NOTICES_ALL" />
        	<cmd id="UPDATE_PROFILE" />
        	<cmd id="DEBUG">
            	<p id="" editor="DEBUG" init="10"/>
        	</cmd>
		</accepts>
	</cmds>
    </nodeDef>

    <nodeDef id="zone" nodeType="139" nls="zone">
        <editors />
        <sts>
		<st id="ST" editor="power" />
		<st id="GV0" editor="source" />
		<st id="SVOL" editor="vol" />
		<st id="GV2" editor="zone_info" />
		<st id="GV3" editor="zone_info" />
		<st id="GV4" editor="zone_info" />
		<st id="GV5" editor="power" />
		<st id="GV6" editor="power" />
		<st id="GV7" editor="power" />
	</sts>
    <cmds>
		<sends>
			<cmd id="DON" />
			<cmd id="DOFF" />
			<cmd id="DIM" /> <!-- minus -->
			<cmd id="BRT" /> <!-- plus -->
			<cmd id="GV12" /> <!-- volup -->
			<cmd id="GV13" /> <!-- voldown -->
			<cmd id="GV14" /> <!-- source -->
			<cmd id="GV15" /> <!-- rev -->
			<cmd id="GV16" /> <!-- fwd -->
			<cmd id="GV17" /> <!-- play -->
			<cmd id="GV18" /> <!-- f1 -->
			<cmd id="GV19" /> <!-- f2 -->
		</sends>
		<accepts>
			<cmd id="HOME" />
			<cmd id="REV" />
			<cmd id="FWD" />
			<cmd id="PLAY" />
			<cmd id="SELECT" />
			<cmd id="LEFT" />
			<cmd id="RIGHT" />
			<cmd id="DOWN" />
			<cmd id="UP" />
			<cmd id="BACK" />
			<cmd id="REPLAY" />
			<cmd id="INFO" />
			<cmd id="BACKSPACE" />
			<cmd id="SEARCH" />
			<cmd id="ENTER" />
		</accepts>
	</cmds>
    </nodeDef>

</nodeDefs>
